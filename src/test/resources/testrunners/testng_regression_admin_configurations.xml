<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Admin automation regression suite" thread-count="3" parallel="none" verbose="3">

	<listeners>
		<listener class-name="com.qa.opencart.testlisteners.ExtentReportListener" />
		<listener class-name="com.qa.opencart.testlisteners.TestAllureListener" />
		<listener class-name="com.qa.opencart.testlisteners.TesultsListener" />
	</listeners>
	
	<test name="Admin delete fees head page Test_Chrome_85">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="85" />
		<classes>
			<class name="com.qa.opencart.tests.DeleteFeesHeadPageTest" />
		</classes>
	</test>
	
	<test name="Admin delete fees term page Test_Chrome_85">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="85" />
		<classes>
			<class name="com.qa.opencart.tests.DeleteFeesTermPageTest" />
		</classes>
	</test>
	
	<test name="Admin delete fees group page Test_Chrome_85">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="85" />
		<classes>
			<class name="com.qa.opencart.tests.DeleteFeesGroupPageTest" />
		</classes>
	</test>
	
	<test name="Admin delete fees structure page Test_Chrome_85">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="85" />
		<classes>
			<class name="com.qa.opencart.tests.DeleteFeesStructurePageTest" />
		</classes>
	</test>
	
	<test name="Admin delete bank master page Test_Chrome_85">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="85" />
		<classes>
			<class name="com.qa.opencart.tests.DeleteBankMasterPageTest" />
		</classes>
	</test>

</suite>	