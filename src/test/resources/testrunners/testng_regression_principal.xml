			<class name="com.qa.opencart.tests.EditARSPageTest" />
		</classes>
	</test>
	
	<test name="Principal edit announcement page Test_Chrome_85">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="85" />
		<classes>
			<class name="com.qa.opencart.tests.EditAnnouncementPageTest" />
		</classes>
	</test>
	
	<test name="Principal edit online video page Test_Chrome_85">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="85" />
		<classes>
			<class name="com.qa.opencart.tests.EditOnlineVideoPageTest" />
		</classes>
	</test>
	
	<test name="Principal edit IM page Test_Chrome_85">
		<parameter name="browser" value="chrome" />
